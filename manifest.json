{
    "name": "AdvancedSearch for YouTube",
    "version": "1.1",
    "description": "Adds more filters to the Search.",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["*://*.youtube.com/*"],
          "js": ["./video_tools/sapisidhash.js",
                "./video_tools/current_cfg_data.js",
                "./video_tools/video_request.js",
                "./upload_functions.js",
                "./age_restricted_check_function.js",
                "./utilities.js",
                "./workaround_css_add.js",
                "./filters.js"],
          "css": ["./as_style.css"],
          "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "128": "./icon.png"
    }
}